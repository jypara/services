{% extends 'base.html.twig' %}

{% block body %}
    <h1>Imagetable</h1>

    <table>
        <tbody>
            <tr>
                <th>Src</th>
                <td>{{ imageTable.src }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ imageTable.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('imagetable_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('imagetable_edit', { 'id': imageTable.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
